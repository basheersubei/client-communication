<%= form_for :user, url: users_path do |f| %>
  
  <p>
    User <%= f.label :name %><br>
    <%= f.text_area :name %>
  </p>

  <p>
    User <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>

  <p>
    User <%= f.label :project_name %><br>
    <%= f.text_area :project_name %>
  </p>

  <p>
    User <%= f.label :project_deadline %><br>
    <%= f.date_field :project_deadline %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>